<div id="title">Tickets</div>
<hr>
<table width="100%">
  <th>Title</th>
  <th>Description</th>
  <th>Actions</th>
  <% @tickets.each do |t| %>
    <tr>
      <td><center><%= t.title %></center></td>
      <td><center><%= t.description %></center></td>
      <td><center>Show | Edit | Delete</center></td>
    </tr>
  <% end %>
</table>
